<h1>Paper Reviews</h1>
<table>
<% Review.all.each do |r| %>
	<tr>
		<td><%= r.id %></td>
		<td><%= r.paper_title %></td>
		<td><%= r.author %></td>
		<td><%= r.review_date %></td>
		<td><%= link_to "Show", r %></td>
		<td><%= link_to "Edit", edit_review_path(r) %></td>
		<td><%= link_to "Delete", r, :method => :delete %></td>
	</tr>
<%end%>
<% if Review.all.empty? %>
	<em> No reviews found </em>
<% end %>
</table>
